{
  "name": "figma-mcp-plugin",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "typescript": "^5.5.0",
    "@figma/plugin-typings": "^1.69.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force dist -ErrorAction SilentlyContinue\"",
    "clean:unix": "rm -rf dist",
    "dev": "powershell -Command \"npm run clean; npm run watch\"",
    "prod": "powershell -Command \"npm run clean; npm run build\""
  }
}